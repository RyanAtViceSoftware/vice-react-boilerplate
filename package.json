{
  "name": "create-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "history": "^4.7.2",
    "localStorage": "^1.0.4",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-thunk": "^2.3.0",
    "rrc": "^0.10.1"
  },
  "scripts": {
    "start": "concurrently --kill-others \"react-scripts start\" \"npm run prettier-watch\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "pretty": "prettier --write \"src/**/*.js\"",
    "prettier-watch": "onchange \"src/**/*.js\" -- ./node_modules/.bin/prettier --write {{changed}}"
