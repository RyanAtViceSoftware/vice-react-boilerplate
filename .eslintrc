{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "comma-spacing": ["error", { "before": false, "after": true }],
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "func-names": "off",
    "prefer-arrow-callback":"off",
    "no-use-before-define": "off",
    "max-len": [2, 80, 4, {"ignoreUrls": true}]
  }
}